 [HttpPost]
 public IActionResult UploadImage(List<IFormFile> files)
 {
     if (files == null || files.Count == 0)
     {
         TempData["Message"] = "No files selected for upload!";
         return RedirectToAction("Index");
     }

     foreach (var file in files)
     {
         if (file.Length > 0)
         {
             // Extract AccountNumber and DPCode from the filename
             var fileName = Path.GetFileNameWithoutExtension(file.FileName);
             var parts = fileName.Split('-');
             if (parts.Length == 2)
             {
                 var accountNumber = parts[0];
                 var dpCode = parts[1];

                 // Ensure the folder path exists
                 string folderPath = Path.Combine("D:\\UPLOAD", dpCode, accountNumber);
                 Directory.CreateDirectory(folderPath);

                 // Save the file
                 string filePath = Path.Combine(folderPath, file.FileName);
                 using (var stream = new FileStream(filePath, FileMode.Create))
                 {
                     file.CopyTo(stream);
                 }

                 // Save file record
                 helper.SaveFileRecord(dpCode, accountNumber);
             }
             else
             {
                 TempData["Message"] = $"Invalid filename format: {file.FileName}";
                 return RedirectToAction("Index");
             }
         }
     }

     TempData["Message"] = "Files uploaded successfully!";
     return RedirectToAction("UploadImage");
 }


