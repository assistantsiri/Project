                <script>
                    function addFileInput() {
                        var div = document.createElement("div");
                        div.innerHTML = '<input type="file" name="files" class="form-control mb-2" accept=".jpg,.jpeg,.png" required />';
                        document.getElementById("fileInputs").appendChild(div);
                    }

                    // 🔍 Search Functionality
                    function searchTable() {
                        var input, filter, table, tr, td, i, txtValue;
                        input = document.getElementById("searchInput");
                        filter = input.value.toUpperCase();
                        table = document.getElementById("tableBody");
                        tr = table.getElementsByTagName("tr");

                        for (i = 0; i < tr.length; i++) {
                            td = tr[i].getElementsByTagName("td")[2]; // Account Number column
                            if (td) {
                                txtValue = td.textContent || td.innerText;
                                if (txtValue.toUpperCase().indexOf(filter) > -1) {
                                    tr[i].style.display = "";
                                } else {
                                    tr[i].style.display = "none";
                                }
                            }
                        }
                    }

                    // 📄 Pagination Functionality
                    var currentPage = 1;
                    var rowsPerPage = 10;

                    function showPage(page) {
                        var table = document.getElementById("tableBody");
                        var tr = table.getElementsByTagName("tr");
                        var totalPages = Math.ceil(tr.length / rowsPerPage);

                        if (page < 1) page = 1;
                        if (page > totalPages) page = totalPages;

                        for (var i = 0; i < tr.length; i++) {
                            tr[i].style.display = "none";
                        }

                        for (var i = (page - 1) * rowsPerPage; i < page * rowsPerPage && i < tr.length; i++) {
                            tr[i].style.display = "";
                        }

                        document.getElementById("pageNumber").innerText = "Page " + page + " of " + totalPages;
                        document.getElementById("prevBtn").disabled = page == 1;
                        document.getElementById("nextBtn").disabled = page == totalPages;
                    }

                    function nextPage() {
                        currentPage++;
                        showPage(currentPage);
                    }

                    function prevPage() {
                        currentPage--;
                        showPage(currentPage);
                    }

                    
                    document.addEventListener("DOMContentLoaded", function () {
                        showPage(1);
                    });
                </script>



                

