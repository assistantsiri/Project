public void SaveFileRecord(string DPCode, string AccountNumber)
{
    var connectionString = GetConfig();
    using (SqlConnection conn = new SqlConnection(connectionString))
    {
        conn.Open();
        string query = @"
        IF NOT EXISTS (SELECT 1 FROM FileUploads WHERE DPCode = @DPCode AND AccountNumber = @AccountNumber)
        INSERT INTO FileUploads (DPCode, AccountNumber) VALUES (@DPCode, @AccountNumber)";

        using (SqlCommand cmd = new SqlCommand(query, conn))
        {
            cmd.Parameters.AddWithValue("@DPCode", DPCode);
            cmd.Parameters.AddWithValue("@AccountNumber", AccountNumber);
            cmd.ExecuteNonQuery();
        }
    }
}



